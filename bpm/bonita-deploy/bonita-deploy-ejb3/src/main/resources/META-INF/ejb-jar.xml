<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd"
	version="3.0">

	<display-name>Bonita Application</display-name>

	<enterprise-beans>
		<session>
			<ejb-name>serverAPIBean</ejb-name>
			<mapped-name>serverAPI</mapped-name>
			<business-remote>org.bonitasoft.engine.api.internal.ServerAPI</business-remote>
			<ejb-class>org.bonitasoft.engine.api.bean.ServerAPIBean</ejb-class>
			<session-type>Stateless</session-type>
      		<transaction-type>Bean</transaction-type>
		</session>
	</enterprise-beans>

</ejb-jar>